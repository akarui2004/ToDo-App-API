{
  "name": "todoapi",
  "version": "1.0.0",
  "description": "This is a todos application API.",
  "main": "src/server.ts",
  "scripts": {
    "script": "ts-node -r tsconfig-paths/register",
    "clean": "rimraf ./dist",
    "build": "tsc --project ./tsconfig.json && tscpaths --silent -p ./tsconfig.json -s ./ -o ./dist",
    "start": "nodemon -r tsconfig-paths/register src/server.ts"
  },
  "keywords": [
    "todos",
    "API"
  ],
  "author": "Minh<minhdpc@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/express-list-endpoints": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-list-endpoints": "^6.0.0",
    "express-validator": "^6.15.0",
    "helmet": "^6.0.1",
    "mysql2": "^3.1.2",
    "nodemon": "^2.0.20",
    "pug": "^3.0.2",
    "remove": "^0.1.5",
    "sequelize": "^6.28.0",
    "sequelize-cli": "^6.6.0",
    "toml": "^3.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.0",
    "@types/pug": "^2.0.6",
    "typescript": "^4.9.5"
  },
  "nodemonConfig": {
    "ext": "ts,toml",
    "execMap": {
      "ts": "ts-node"
    },
    "ignore": [
      "node_modules/**/*",
      "scripts/**/*"
    ],
    "delay": "300"
  }
}
